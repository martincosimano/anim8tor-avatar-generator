{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://animated-avatar-generator.onrender.com/api/$1",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      },
      {
        "src": "/(.*)",
        "dest": "/client/$1",
        "status": 200
      }
    ]
}